            <Label minHeight="-Infinity" minWidth="-Infinity" prefHeight="15.0" prefWidth="53.0" text="Phone   " HBox.hgrow="NEVER" />
            <TextField fx:id="phone" prefHeight="25.0" prefWidth="814.0" HBox.hgrow="ALWAYS" />
